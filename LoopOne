LoopOne :

/**
 * Amanda Torres
 * September 09 ,2016
 * LoopOne lab
 */
public class LoopOne
{
    int[] myLoop = new int[10];

    public LoopOne()
    {
    }

    public int loadArray(int[] myArray)
    {
        int number = 0;
        for(int i = 0; i < myLoop.length; i++){
            number = number + myArray[i];
        }
        return number;
    }
}

